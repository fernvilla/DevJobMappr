<%- if controller_name != 'sessions' %>
  <%= link_to_modal "Sign in", new_session_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to_modal "Sign up", new_registration_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to_modal "Forgot your password?", new_password_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to_modal "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to_modal "Didn't receive unlock instructions?", new_unlock_path(resource_name) %><br />
<% end -%>

<br>
<%= link_to image_tag("linkedin.jpg"), user_omniauth_authorize_path(:linkedin), :class=>"logo" %>
<br><br>
<%= link_to image_tag("twitter.jpg"), user_omniauth_authorize_path(:twitter), :class=>"logo" %>
